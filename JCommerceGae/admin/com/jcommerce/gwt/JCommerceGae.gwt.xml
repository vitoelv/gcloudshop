<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.6.4//EN" "http://google-web-toolkit.googlecode.com/svn/tags/1.6.4/distro-source/core/src/gwt-module.dtd">
<module rename-to='jcommercegae'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>
      <inherits name="com.google.gwt.core.Core"/>
      <inherits name="com.google.gwt.i18n.I18N"/>
	  <inherits name="com.google.gwt.http.HTTP"/>
	  <inherits name='com.extjs.gxt.ui.GXT'/>
  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <inherits name='com.google.gwt.user.theme.standard.Standard'/>
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

  <!-- Other module inherits                                      -->
<inherits name='com.extjs.gxt.ui.GXT'/>


  <!-- Specify the app entry point class.                         -->
  <entry-point class='com.jcommerce.gwt.client.JCommerceGae'/>
  
        <!-- Internationalization support. -->
      <extend-property name="locale" values="en"/>
      <extend-property name="locale" values="zh"/>
    
      <!-- Specify the application specific style sheet.              -->
      <stylesheet src='iShop.css' />
  <servlet path="/ishopService.do" class="com.jcommerce.gwt.server.IShopServiceImpl" />		
  
  <servlet path="/customizedService.do" class="com.jcommerce.gwt.server.CustomizedServiceImpl" />
  <servlet path="/gwtHttpService.do" class="com.jcommerce.gwt.server.GWTHttpServlet" />
  <servlet path="/dynaImageService.do" class="example.DynaImageServlet" />
  

</module>
